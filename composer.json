{
    "name": "tomcodes/laravel-calendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "tomcodes",
            "email": "tomcodes@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "~7.0|~8.0|~9.0"
    },
    "autoload": {
